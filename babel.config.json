{
    "presets": [
        ["@babel/preset-env", {"modules": false}], 
        "@babel/preset-react"
    ],
    "plugins": [
        [
          "babel-plugin-root-import",
          {
            "rootPathSuffix": "./",
            "rootPathPrefix": "~/"
          }
        ],
        ["inline-dotenv",{
          "path": ".env"
        }]
      ]
}