

RedirectMatch 302 "^/eat-drink$" https://thebierelibrary.ocdla.org/home
RedirectMatch 302 "^/eat-drink/food$" https://thebierelibrary.ocdla.org/food
RedirectMatch 302 "^/eat-drink/bottlescans-draft$" https://thebierelibrary.ocdla.org/drink
RedirectMatch 302 "^/eat-drink/bottlescans-drafts$" https://thebierelibrary.ocdla.org/drink
RedirectMatch 302 "^/eat-drink/bottles-drafts$" https://thebierelibrary.ocdla.org/drink
RedirectMatch 302 "^/eat-drink/cans-drafts$" https://thebierelibrary.ocdla.org/drink
RedirectMatch 302 "^/eat-drink/bottles$" https://thebierelibrary.ocdla.org/drink
RedirectMatch 302 "^/eat-drink/cans$" https://thebierelibrary.ocdla.org/drink
# Redirect 302 / https://thebierelibrary.ocdla.org/home

<IfModule mod_rewrite.c>
    Options -Indexes

    RewriteEngine On
    RewriteBase /

    # When using a reverse proxy,
    # These need to be commented out
    # as the SSL terminator is on another machine
    # and all requests are made using standard HTTP.
    # RewriteCond %{SERVER_PORT} 80
    # RewriteRule ^(.*)$ https://thebierelibrary.ocdla.org/$1 [R,L]

    RewriteCond %{HTTP_HOST} ^www\.thebierelibrary\.ocdla.org$ [NC]
    RewriteRule ^(.*)$ https://thebierelibrary.ocdla.org/$1 [R,L]

	RewriteRule ^$ /home [R,L]

    RewriteCond %{REQUEST_URI} !\.(css|js|php|html|png|jpg|jpeg|gif|ico|pdf|svg|webp)$ [NC]
    RewriteCond %{REQUEST_URI} !manifest$ [NC]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . index.php [L]
</IfModule>



<IfModule mod_headers.c>
	Header set Access-Control-Allow-Origin "*"
	Header set Access-Control-Allow-Methods "POST, GET, OPTIONS, DELETE, PUT"
	Header set Access-Control-Max-Age "1000"
	Header set Access-Control-Allow-Headers "x-requested-with, Content-Type, origin, authorization, accept, client-security-token"
</IfModule>
